 //暂时性解决dialog 问题 https://github.com/ElemeFE/element/issues/2461
 .el-dialog {
   transform: none;
   position: relative;
   margin:0 30px;
   height:auto;
 }
 .popper__arrow{
   left: 63px!important;
   top: -7px!important;
 }

 //文章页textarea修改样式
 .article-textarea {
   textarea {
     padding-right: 40px;
     resize: none;
     border: none;
     border-radius: 0px;
     border-bottom: 1px solid #bfcbd9;
   }
 }
 .filter-container .el-button, .filter-input .el-button{
  background:#e33434;
  border:none;
  &:hover{
    background:#ff4d4d;
  }
  &:active{
    background:#e33434;
  }
 }
 //element ui upload
 .upload-container {
   .el-upload {
     width: 100%;
     .el-upload-dragger {
       width: 100%;
       height: 200px;
     }
   }
 }
.el-textarea__inner{
  writing-mode:vertical-lr;
    display:table-cell;
  }
  .el-button span{
    font-family: 'OrhonChaganTig';
    writing-mode:vertical-lr;
    -webkit-text-orientation: sideways-right;
  }
//设置
 .collapse-button{
   writing-mode:vertical-lr;
   border: none;
 .el-collapse-item__header{
   height: 100%;
   border: none;
 }}
 .el-collapse {
   border: none!important;
 }
 .filters-input{
   height: 100%;
   .el-input{
     height: 100%;
     .el-input__inner{
       height: 100%!important;
     }
   }
 }
 .el-tree__empty-text{
   top: 100%!important;
 }
 .el-tree{
   margin-left: 40px;
   writing-mode:horizontal-tb;
   span{
     writing-mode:horizontal-tb;
   }
 }
