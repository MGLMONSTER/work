.links_block {
    background: #f8fafc;
    box-shadow: 0 0 5px #ccc;
    padding: 20px;
    height: 340px;
    margin-bottom: 30px;
}
.links_block h2.links_title {
    font-size: 30px;
    padding: 0 0 0 10px;
    width: 50px;
    color: #708da7;
    font-weight: normal;
    font-style: initial;
    border-right: 3px solid #708da7;
    margin-right: 15px;
}
.links_block .row.orhon_mgl {
    width: 100%;
    margin: 0;
}
.orhon-demon ul.links {
    height: 300px;
    width: calc( 100% - 70px );
    margin-bottom: 0px;
    overflow-x: auto;
}

.orhon-demon .links h2 {
    background: #d9e7f4;
    color: #708da7;
    line-height: 50px;
    text-align: left;
    display: block;
    padding: 20px 0px;
    margin-right: 10px;
}

.orhon-demon .links li.friedly_links {
    height: 100%;
    width: 50px;
    margin: 0 8px 0 0px;
    /* padding: 10px 0; */
}

.orhon-demon .links li a {
    display: block;
    width: 100%;
    min-height: 100px;
    margin: 0px auto;
    background: #eaf1f4 url(../img/links-bg.jpg) no-repeat bottom center;
    border: 1px solid #eee;
    text-decoration: none;
    color: #708da7;
    padding: 3px 0px;
    border-radius: 2px;
}

.orhon-demon .links li a:hover {
    color: #db8d3d;
}

.orhon-demon .links li a img {
    width: 100%;
    max-width: 120px;
    max-height: 120px;
    margin: 0px auto;
    display: inline-block;
    float: left;
    border-radius: 50%;
}

.orhon-demon .links li a .links-name {
    display: block;
    width: 100%;
    height: calc( 100% - 60px);
    margin: 0px auto;
    margin-bottom: 50px;
    overflow: hidden;
    white-space: nowrap;
    padding: 7px 11px;
}
.orhon-demon .resource-search {
    width: 120px;
    height: 252px;
}

.orhon-demon .resource-search h2 {
    width: 50px;
    height: 252px;
    background: #d9e7f4;
    color: #708da7;
    line-height: 50px;
    text-align: left;
    display: block;
    float: right;
    margin: 0px;
    padding: 20px 0px;
    margin-right: 20px;
}

.orhon-demon .resource-search .search {
    width: 50px;
    height: 252px;
    background: #fff;
    float: right;
    position: relative;
}

.orhon-demon .resource-search .search .search-field {
    width: 50px;
    height: 201px;
    color: #2b2b2b;
    font-family: OrhonChaganTig, Lato, sans-serif;
    font-size: 18px;
    font-weight: normal;
    line-height: 1.5;
    border: 0px;
    vertical-align: baseline;
    display: table-cell;
    padding: 5px;
}

.orhon-demon .resource-search .search .btn {
    width: 50px;
    height: 50px;
    border-top: 1px solid #829cb3;
    background: none;
    text-align: center;
    font-size: 25px;
    border-radius: 0px;
    display: table-cell;
    position: absolute;
    bottom: 0;
    left: 0;
    padding: 0;
}

.orhon-demon .resource-search .search .btn button {
