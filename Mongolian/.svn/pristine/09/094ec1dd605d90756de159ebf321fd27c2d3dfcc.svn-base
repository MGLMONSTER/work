.orhon-demon .features .overlay-figure,
.orhon-demon .thematic .overlay-figure {
    position: relative;
    display: table;
    border: solid 1px #d3e4e7;
}

.orhon-demon .features .overlay-figure .overlay-figure-content,
.orhon-demon .thematic .overlay-figure .overlay-figure-content {
    width: 100%;
    height: 100%;
    -webkit-transform: scale(1) translateZ(0);
    -moz-transform: scale(1) translateZ(0);
    transform: scale(1) translateZ(0);
    -webkit-transition: -webkit-transform 0.5s ease 0.25s;
    -moz-transition: -moz-transform 0.5s ease 0.25s;
    transition: transform 0.5s ease 0.25s;
}

.orhon-demon .features .overlay-figure .overlay-figure-content img,
.orhon-demon .thematic .overlay-figure .overlay-figure-content img {
    display: block;
}

.orhon-demon .features .overlay-figure .overlay-figure-content .overlay-figure-background,
.orhon-demon .thematic .overlay-figure .overlay-figure-content .overlay-figure-background {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    opacity: 0;
    background: rgba(13, 120, 114, 0.5);
}

.orhon-demon .features .overlay-figure:hover .overlay-figure-content,
.orhon-demon .thematic .overlay-figure:hover .overlay-figure-content {
    -webkit-transform: scale(0.95) translateZ(0);
    -moz-transform: scale(0.95) translateZ(0);
    transform: scale(0.95) translateZ(0);
}

.orhon-demon .features .overlay-figure:hover .overlay-figure-background,
.orhon-demon .thematic .overlay-figure:hover .overlay-figure-background {
    opacity: 1;
}

.orhon-demon .features .overlay-figure .overlay-figure-caption,
.orhon-demon .thematic .overlay-figure .overlay-figure-caption {
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    font-family: adobe-clean, 'Helvetica Neue', Helvetica, Arial, sans-serif;
    color: white;
    opacity: 0;
    -webkit-transition: opacity ease 0.25s;
    -moz-transition: opacity ease 0.25s;
    transition: opacity ease 0.25s;
}

.orhon-demon .features .overlay-figure .overlay-figure-caption .contents,
.orhon-demon .thematic .overlay-figure .overlay-figure-caption .contents {
    display: table;
    width: 100%;
    height: 100%;
}

.orhon-demon .features .overlay-figure .overlay-figure-caption .contents .contents-stack,
.orhon-demon .thematic .overlay-figure .overlay-figure-caption .contents .contents-stack {
    display: table-cell;
    vertical-align: middle;
    text-align: center;
    padding: 5%;
}

.orhon-demon .features .overlay-figure .overlay-figure-caption .title,
.orhon-demon .thematic .overlay-figure .overlay-figure-caption .title {
    width: 150px;
    height: 100px;
    overflow: hidden;
    display: block;
    margin: 0 auto;
    font-size: 20px;
    opacity: 0;
    -webkit-transform: translateY(-100%) translateZ(0);
    -moz-transform: translateY(-100%) translateZ(0);
    transform: translateY(-100%) translateZ(0);
    -webkit-transition: -webkit-transform 0.5s ease 0.25s, opacity 0.15s ease 0.25s;
    -moz-transition: -moz-transform 0.5s ease 0.25s, opacity 0.15s ease 0.25s;
    transition: transform 0.5s ease 0.25s, opacity 0.15s ease 0.25s;
}

.orhon-demon .features .overlay-figure .overlay-figure-caption .title a,
.orhon-demon .thematic .overlay-figure .overlay-figure-caption .title a {
    text-decoration: none;
    color: #fff;
}

.orhon-demon .features .overlay-figure .overlay-figure-caption .text,
.orhon-demon .thematic .overlay-figure .overlay-figure-caption .text {
    display: block;
    margin-top: 0;
    opacity: 0;
    -webkit-transform: translateY(100%) translateZ(0);
    -moz-transform: translateY(100%) translateZ(0);
    transform: translateY(100%) translateZ(0);
    -webkit-transition: -webkit-transform 0.5s ease 0.25s, opacity 0.15s ease 0.25s;
    -moz-transition: -moz-transform 0.5s ease 0.25s, opacity 0.15s ease 0.25s;
    transition: transform 0.5s ease 0.25s, opacity 0.15s ease 0.25s;
}

.orhon-demon .features .overlay-figure .separator,
.orhon-demon .thematic .overlay-figure .separator {
    display: block;
    margin: 12px;
    margin-left: auto;
    margin-right: auto;
    height: 2px;
    width: 30%;
    background-color: white;
    -webkit-transform: scaleX(0) translateZ(0);
    -moz-transform: scaleX(0) translateZ(0);
    transform: scaleX(0) translateZ(0);
    -webkit-transition: -webkit-transform 0.25s ease 0.25s;
    -moz-transition: -moz-transform 0.25s ease 0.25s;
    transition: transform 0.25s ease 0.25s;
}

.orhon-demon .features .overlay-figure:hover .overlay-figure-caption,
.orhon-demon .thematic .overlay-figure:hover .overlay-figure-caption {
    opacity: 1;
}

.orhon-demon .features .overlay-figure:hover .title,
