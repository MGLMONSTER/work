}

.orhon-demon .detail-main ul.vjs-menu-content{
    padding: 0;
}
.orhon-demon .detail-main .vjs-menu li{
    list-style: none;
    margin: 0;
    padding: 0.2em 0;
    line-height: 1.4em;
    font-size: 1.2em;
    text-align: center;
    text-transform: lowercase;
    margin-top: 0;
}
.res_name_title {
    position: absolute;
    left: 0;
    z-index: 1000;
    writing-mode: vertical-lr;
    color: #ffd100;    
    background: rgba(255, 255, 255, 0.58);
    height: calc( 100% - 30px );
    height: -moz-calc( 100% - 30px );
    height: -webkit-calc( 100% - 30px );
    padding: 15px;
}
.res_name_title{
    writing-mode: vertical-lr;
    -webkit-writing-mode: vertical-lr;
    -o-writing-mode: vertical-lr;
    -ms-writing-mode: tb-lr;
    -moz-writing-mode: vertical-lr;
    font-family: "OrhonChaganTig";
    unicode-bidi: embed;
    text-orientation: sideways-right;
    -webkit-text-orientation: sideways-right;
    -mz-text-orientation: sideways-right;
    -o-text-orientation: sideways-right;
    -ms-text-orientation: sideways-right;
    word-spacing: normal;
    word-wrap: normal;
    word-break: keep-all;
}
.detail-main .tools {
    display: inline-block;
    position: absolute;
    right: 0;
    top: 0;
    z-index: 10000;
    background: rgba(92, 149, 197, 0.8);
    padding: 6px 10px;
    border-radius: 2px;
}
.detail-main .tools span.glyphicon{
  color: #fff;
  cursor: pointer;
}
.detail-main .tools span.color{
    color: #ff8800;
    cursor: pointer;
}
.detail-main .tools a{
    margin: 0 6px;
}
.res_info_box {
    display: inline-block;
    width: 100%;
}
.resouce_play.detail-main.others {
    display: inline-block;
    width: 50%;
    float: left;
    background: url("../img/other_attechment.png");
